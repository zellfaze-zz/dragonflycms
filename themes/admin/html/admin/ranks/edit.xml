
<h1 i18n:translate="">Ranks_title</h1>

<p i18n:translate="">Ranks_explain</p>

<form action="" method="post">
<table><thead>
	<tr>
		<th colspan="3" i18n:translate="">Ranks_title</th>
	</tr>
</thead><tbody>
	<tr>
		<td style="width:38%" i18n:translate="">Rank_title</td>
		<td><input type="text" name="title" size="35" maxlength="40" tal:attributes="value RANK_TITLE"/></td>
	</tr>
	<tr>
		<td i18n:translate="">Rank_special</td>
		<td><input type="checkbox" name="rank_special" value="1" tal:attributes="checked RANK_SPECIAL"/></td>
		<td></td>
	</tr>
	<tr>
		<td i18n:translate="">Rank_minimum</td>
		<td><input type="number" name="rank_min" min="0" size="5" maxlength="10" tal:attributes="value RANK_MINIMUM"/></td>
		<td></td>
	</tr>
	<tr>
		<td i18n:translate="">Rank_image</td>
		<td><input type="text" name="rank_image" size="40" maxlength="255" tal:attributes="value RANK_IMAGE"/>
			<div tal:condition="RANK_IMAGE"><img tal:attributes="src RANK_IMAGE" alt=""/></div>
		</td>
		<td><i class="infobox"><span i18n:translate="">Rank_image_explain</span></i></td>
	</tr>
</tbody><tfoot>
	<tr>
		<td colspan="3">
			<button name="save" i18n:translate="">Save</button>
			<button type="reset" i18n:translate="">Reset</button>
		</td>
	</tr>
</tfoot></table>
</form>
